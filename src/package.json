{
	"name": "roo-cline-bitx",
	"displayName": "%extension.displayName%",
	"description": "%extension.description%",
	"publisher": "RooVeterinaryInc",
	"version": "0.1.0",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0",
		"node": "20.19.2"
	},
	"author": {
		"name": "Bitx"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/RooCodeInc/Bitx"
	},
	"homepage": "https://roocode.com",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"Bitx",
		"roocode"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "roo-cline-bitx-ActivityBar",
					"title": "%views.activitybar.title%",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"roo-cline-bitx-ActivityBar": [
				{
					"type": "webview",
					"id": "roo-cline-bitx.SidebarProvider",
					"name": "%views.sidebar.name%"
				}
			]
		},
		"commands": [
			{
				"command": "roo-cline-bitx.plusButtonClicked",
				"title": "%command.newTask.title%",
				"icon": "$(edit)"
			},
			{
				"command": "roo-cline-bitx.historyButtonClicked",
				"title": "%command.history.title%",
				"icon": "$(history)"
			},
			{
				"command": "roo-cline-bitx.marketplaceButtonClicked",
				"title": "%command.marketplace.title%",
				"icon": "$(extensions)"
			},
			{
				"command": "roo-cline-bitx.popoutButtonClicked",
				"title": "%command.openInEditor.title%",
				"icon": "$(link-external)"
			},
			{
				"command": "roo-cline-bitx.cloudButtonClicked",
				"title": "%command.cloud.title%",
				"icon": "$(cloud)"
			},
			{
				"command": "roo-cline-bitx.settingsButtonClicked",
				"title": "%command.settings.title%",
				"icon": "$(settings-gear)"
			},
			{
				"command": "roo-cline-bitx.worktreesButtonClicked",
				"title": "%command.worktrees.title%",
				"icon": "$(git-branch)"
			},
			{
				"command": "roo-cline-bitx.openInNewTab",
				"title": "%command.openInNewTab.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.explainCode",
				"title": "%command.explainCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.fixCode",
				"title": "%command.fixCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.improveCode",
				"title": "%command.improveCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.addToContext",
				"title": "%command.addToContext.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.newTask",
				"title": "%command.newTask.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.terminalAddToContext",
				"title": "%command.terminal.addToContext.title%",
				"category": "Terminal"
			},
			{
				"command": "roo-cline-bitx.terminalFixCommand",
				"title": "%command.terminal.fixCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "roo-cline-bitx.terminalExplainCommand",
				"title": "%command.terminal.explainCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "roo-cline-bitx.setCustomStoragePath",
				"title": "%command.setCustomStoragePath.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.importSettings",
				"title": "%command.importSettings.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.focusInput",
				"title": "%command.focusInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.acceptInput",
				"title": "%command.acceptInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-cline-bitx.toggleAutoApprove",
				"title": "%command.toggleAutoApprove.title%",
				"category": "%configuration.title%"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "roo-cline-bitx.contextMenu",
					"group": "1"
				}
			],
			"roo-cline-bitx.contextMenu": [
				{
					"command": "roo-cline-bitx.addToContext",
					"group": "1_actions@1"
				},
				{
					"command": "roo-cline-bitx.explainCode",
					"group": "1_actions@2"
				},
				{
					"command": "roo-cline-bitx.improveCode",
					"group": "1_actions@3"
				}
			],
			"terminal/context": [
				{
					"submenu": "roo-cline-bitx.terminalMenu",
					"group": "2"
				}
			],
			"roo-cline-bitx.terminalMenu": [
				{
					"command": "roo-cline-bitx.terminalAddToContext",
					"group": "1_actions@1"
				},
				{
					"command": "roo-cline-bitx.terminalFixCommand",
					"group": "1_actions@2"
				},
				{
					"command": "roo-cline-bitx.terminalExplainCommand",
					"group": "1_actions@3"
				}
			],
			"view/title": [
				{
					"command": "roo-cline-bitx.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == roo-cline-bitx.SidebarProvider"
				},
				{
					"command": "roo-cline-bitx.settingsButtonClicked",
					"group": "navigation@2",
					"when": "view == roo-cline-bitx.SidebarProvider"
				},
				{
					"command": "roo-cline-bitx.cloudButtonClicked",
					"group": "navigation@3",
					"when": "view == roo-cline-bitx.SidebarProvider"
				},
				{
					"command": "roo-cline-bitx.marketplaceButtonClicked",
					"group": "navigation@4",
					"when": "view == roo-cline-bitx.SidebarProvider"
				},
				{
					"command": "roo-cline-bitx.historyButtonClicked",
					"group": "overflow@1",
					"when": "view == roo-cline-bitx.SidebarProvider"
				},
				{
					"command": "roo-cline-bitx.worktreesButtonClicked",
					"group": "overflow@2",
					"when": "view == roo-cline-bitx.SidebarProvider"
				},
				{
					"command": "roo-cline-bitx.popoutButtonClicked",
					"group": "overflow@3",
					"when": "view == roo-cline-bitx.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "roo-cline-bitx.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == roo-cline-bitx.TabPanelProvider"
				},
				{
					"command": "roo-cline-bitx.settingsButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == roo-cline-bitx.TabPanelProvider"
				},
				{
					"command": "roo-cline-bitx.cloudButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == roo-cline-bitx.TabPanelProvider"
				},
				{
					"command": "roo-cline-bitx.marketplaceButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == roo-cline-bitx.TabPanelProvider"
				},
				{
					"command": "roo-cline-bitx.historyButtonClicked",
					"group": "overflow@1",
					"when": "activeWebviewPanelId == roo-cline-bitx.TabPanelProvider"
				},
				{
					"command": "roo-cline-bitx.worktreesButtonClicked",
					"group": "overflow@2",
					"when": "activeWebviewPanelId == roo-cline-bitx.TabPanelProvider"
				},
				{
					"command": "roo-cline-bitx.popoutButtonClicked",
					"group": "overflow@3",
					"when": "activeWebviewPanelId == roo-cline-bitx.TabPanelProvider"
				}
			]
		},
		"keybindings": [
			{
				"command": "roo-cline-bitx.addToContext",
				"key": "cmd+k cmd+a",
				"mac": "cmd+k cmd+a",
				"win": "ctrl+k ctrl+a",
				"linux": "ctrl+k ctrl+a",
				"when": "editorTextFocus && editorHasSelection"
			},
			{
				"command": "roo-cline-bitx.toggleAutoApprove",
				"key": "cmd+alt+a",
				"mac": "cmd+alt+a",
				"win": "ctrl+alt+a",
				"linux": "ctrl+alt+a"
			}
		],
		"submenus": [
			{
				"id": "roo-cline-bitx.contextMenu",
				"label": "%views.contextMenu.label%"
			},
			{
				"id": "roo-cline-bitx.terminalMenu",
				"label": "%views.terminalMenu.label%"
			}
		],
		"configuration": {
			"title": "%configuration.title%",
			"properties": {
				"roo-cline-bitx.allowedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"git log",
						"git diff",
						"git show"
					],
					"description": "%commands.allowedCommands.description%"
				},
				"roo-cline-bitx.deniedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%commands.deniedCommands.description%"
				},
				"roo-cline-bitx.commandExecutionTimeout": {
					"type": "number",
					"default": 0,
					"minimum": 0,
					"maximum": 600,
					"description": "%commands.commandExecutionTimeout.description%"
				},
				"roo-cline-bitx.commandTimeoutAllowlist": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%commands.commandTimeoutAllowlist.description%"
				},
				"roo-cline-bitx.preventCompletionWithOpenTodos": {
					"type": "boolean",
					"default": false,
					"description": "%commands.preventCompletionWithOpenTodos.description%"
				},
				"roo-cline-bitx.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.vendor.description%"
						},
						"family": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.family.description%"
						}
					},
					"description": "%settings.vsCodeLmModelSelector.description%"
				},
				"roo-cline-bitx.customStoragePath": {
					"type": "string",
					"default": "",
					"description": "%settings.customStoragePath.description%"
				},
				"roo-cline-bitx.enableCodeActions": {
					"type": "boolean",
					"default": true,
					"description": "%settings.enableCodeActions.description%"
				},
				"roo-cline-bitx.autoImportSettingsPath": {
					"type": "string",
					"default": "",
					"description": "%settings.autoImportSettingsPath.description%"
				},
				"roo-cline-bitx.maximumIndexedFilesForFileSearch": {
					"type": "number",
					"default": 10000,
					"minimum": 5000,
					"maximum": 500000,
					"description": "%settings.maximumIndexedFilesForFileSearch.description%"
				},
				"roo-cline-bitx.useAgentRules": {
					"type": "boolean",
					"default": true,
					"description": "%settings.useAgentRules.description%"
				},
				"roo-cline-bitx.apiRequestTimeout": {
					"type": "number",
					"default": 600,
					"minimum": 0,
					"maximum": 3600,
					"description": "%settings.apiRequestTimeout.description%"
				},
				"roo-cline-bitx.newTaskRequireTodos": {
					"type": "boolean",
					"default": false,
					"description": "%settings.newTaskRequireTodos.description%"
				},
				"roo-cline-bitx.codeIndex.embeddingBatchSize": {
					"type": "number",
					"default": 60,
					"minimum": 1,
					"maximum": 200,
					"description": "%settings.codeIndex.embeddingBatchSize.description%"
				},
				"roo-cline-bitx.debug": {
					"type": "boolean",
					"default": false,
					"description": "%settings.debug.description%"
				},
				"roo-cline-bitx.debugProxy.enabled": {
					"type": "boolean",
					"default": false,
					"description": "%settings.debugProxy.enabled.description%",
					"markdownDescription": "%settings.debugProxy.enabled.description%"
				},
				"roo-cline-bitx.debugProxy.serverUrl": {
					"type": "string",
					"default": "http://127.0.0.1:8888",
					"description": "%settings.debugProxy.serverUrl.description%",
					"markdownDescription": "%settings.debugProxy.serverUrl.description%"
				},
				"roo-cline-bitx.debugProxy.tlsInsecure": {
					"type": "boolean",
					"default": false,
					"description": "%settings.debugProxy.tlsInsecure.description%",
					"markdownDescription": "%settings.debugProxy.tlsInsecure.description%"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint . --ext=ts --max-warnings=0",
		"check-types": "tsc --noEmit",
		"pretest": "turbo run bundle --cwd ..",
		"test": "vitest run",
		"format": "prettier --write .",
		"bundle": "node esbuild.mjs",
		"vscode:prepublish": "pnpm bundle --production",
		"vsix": "mkdirp ../bin && vsce package --no-dependencies --out ../bin",
		"publish:marketplace": "vsce publish --no-dependencies && ovsx publish --no-dependencies",
		"watch:bundle": "pnpm bundle --watch",
		"watch:tsc": "cd .. && tsc --noEmit --watch --project src/tsconfig.json",
		"clean": "rimraf README.md CHANGELOG.md LICENSE dist logs mock .turbo"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.7.0",
		"@aws-sdk/client-bedrock-runtime": "^3.922.0",
		"@aws-sdk/credential-providers": "^3.922.0",
		"@google/genai": "^1.29.1",
		"@lmstudio/sdk": "^1.1.1",
		"@mistralai/mistralai": "^1.9.18",
		"@modelcontextprotocol/sdk": "1.12.0",
		"@qdrant/js-client-rest": "^1.14.0",
		"@bitx/cloud": "workspace:^",
		"@bitx/core": "workspace:^",
		"@bitx/ipc": "workspace:^",
		"@bitx/telemetry": "workspace:^",
		"@bitx/types": "workspace:^",
		"@vscode/codicons": "^0.0.36",
		"async-mutex": "^0.5.0",
		"axios": "^1.12.0",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"diff-match-patch": "^1.0.5",
		"exceljs": "^4.4.0",
		"fast-deep-equal": "^3.1.3",
		"fast-xml-parser": "^5.0.0",
		"fastest-levenshtein": "^1.0.16",
		"fzf": "^0.5.2",
		"get-folder-size": "^5.0.0",
		"global-agent": "^3.0.0",
		"google-auth-library": "^9.15.1",
		"gray-matter": "^4.0.3",
		"i18next": "^25.0.0",
		"ignore": "^7.0.3",
		"isbinaryfile": "^5.0.2",
		"jwt-decode": "^4.0.0",
		"lodash.debounce": "^4.0.8",
		"mammoth": "^1.9.1",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"node-cache": "^5.1.2",
		"node-ipc": "^12.0.0",
		"ollama": "^0.5.17",
		"openai": "^5.12.2",
		"os-name": "^6.0.0",
		"p-limit": "^6.2.0",
		"p-wait-for": "^5.0.2",
		"partial-json": "^0.1.7",
		"pdf-parse": "^1.1.1",
		"pkce-challenge": "^5.0.0",
		"pretty-bytes": "^7.0.0",
		"proper-lockfile": "^4.1.2",
		"ps-tree": "^1.2.0",
		"puppeteer-chromium-resolver": "^24.0.0",
		"puppeteer-core": "^23.4.0",
		"reconnecting-eventsource": "^1.6.4",
		"safe-stable-stringify": "^2.5.0",
		"sanitize-filename": "^1.6.3",
		"say": "^0.16.0",
		"semver-compare": "^1.0.0",
		"serialize-error": "^12.0.0",
		"shell-quote": "^1.8.2",
		"simple-git": "^3.27.0",
		"socket.io-client": "^4.8.1",
		"sound-play": "^1.1.0",
		"stream-json": "^1.8.0",
		"string-similarity": "^4.0.4",
		"strip-ansi": "^7.1.0",
		"strip-bom": "^5.0.0",
		"tiktoken": "^1.0.21",
		"tmp": "^0.2.3",
		"tree-sitter-wasms": "^0.1.12",
		"turndown": "^7.2.0",
		"undici": "^6.21.3",
		"uuid": "^11.1.0",
		"vscode-material-icons": "^0.1.1",
		"web-tree-sitter": "^0.25.6",
		"workerpool": "^9.2.0",
		"yaml": "^2.8.0",
		"zod": "3.25.61"
	},
	"devDependencies": {
		"@bitx/build": "workspace:^",
		"@bitx/config-eslint": "workspace:^",
		"@bitx/config-typescript": "workspace:^",
		"@types/clone-deep": "^4.0.4",
		"@types/debug": "^4.1.12",
		"@types/diff": "^5.2.1",
		"@types/diff-match-patch": "^1.0.36",
		"@types/glob": "^8.1.0",
		"@types/lodash.debounce": "^4.0.9",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@types/node-cache": "^4.1.3",
		"@types/node-ipc": "^9.2.3",
		"@types/proper-lockfile": "^4.1.4",
		"@types/ps-tree": "^1.1.6",
		"@types/semver-compare": "^1.0.3",
		"@types/shell-quote": "^1.7.5",
		"@types/stream-json": "^1.7.8",
		"@types/string-similarity": "^4.0.2",
		"@types/tmp": "^0.2.6",
		"@types/turndown": "^5.0.5",
		"@types/vscode": "^1.84.0",
		"@vscode/test-electron": "^2.5.2",
		"@vscode/vsce": "3.3.2",
		"esbuild-wasm": "^0.25.0",
		"execa": "^9.5.2",
		"glob": "^11.1.0",
		"mkdirp": "^3.0.1",
		"nock": "^14.0.4",
		"npm-run-all2": "^8.0.1",
		"ovsx": "0.10.4",
		"rimraf": "^6.0.1",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"vitest": "^3.2.3",
		"zod-to-ts": "^1.2.0"
	}
}
